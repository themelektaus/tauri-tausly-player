{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Tausly Player",
    "version": "0.1.3"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": false,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [ "../src/favicon.ico" ],
      "identifier": "com.tausi.tausly-player",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "url": "main.html",
        "title": "Loading...",
        "decorations": true,
        "width": 240,
        "height": 0,
        "resizable": true,
        "fullscreen": false,
        "visible": false
      },
      {
        "label": "splash",
        "url": "splash.html",
        "title": "Tausly Player",
        "decorations": false,
        "width": 300,
        "height": 150,
        "resizable": false,
        "fullscreen": false,
        "visible": false
      }
    ]
  }
}